.item {
    height: 30px;
    border-color: rgb(22, 22, 22);
    border-bottom-width: 1px;
    flex-direction: row;
}

.column {
    min-width: 100px;
    flex-grow: 1;
    -unity-text-align: upper-right;
    border-color: rgb(22, 22, 22);
    border-left-width: 1px;
    padding: 5px 0px 5px 0px;
    margin: 0px;
}
